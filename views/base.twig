<!doctype html>
<html class="no-js" {{ language_attributes }}>
<head>
    {% block head %}
        {% include 'html-header.twig' %}
    {% endblock %}
</head>
<body class="{{ body_class }}" data-source="base.twig">

<div id="fb-root"></div>
<script>(function(d, s, id) {
        var js, fjs = d.getElementsByTagName(s)[0];
        if (d.getElementById(id)) return;
        js = d.createElement(s); js.id = id;
        js.src = "//connect.facebook.net/en_US/all.js#xfbml=1&appId=283384575022710";
        fjs.parentNode.insertBefore(js, fjs);
    }(document, 'script', 'facebook-jssdk'));</script>

<!--[if lt IE 9]>
    <p class="browsehappy">It looks like you are using an outdated browser.<br />Please <a href="http://browsehappy.com/">upgrade your browser</a> to fully enjoy the NationSwell experience.</p>
<![endif]-->

{% include 'panel-more-stories.twig' %}

<div id="page-wrapper">
    {% include 'nav.twig' %}

    {% block topics %}
    {% include 'trending-topics.twig' %}
    {% endblock %}

    <main id="page-content" role="main">
        {% block content %}
            Sorry, no content
        {% endblock %}
    </main>

    {% include 'footer.twig' %}
</div>

{% block modals %}
    {% if modal_joinus_enabled is not empty and is_editor == false %}
        {% include 'modal-joinus.twig' %}
    {% endif %}

    {% include 'modal.twig' %}
{% endblock %}

{{ wp_footer }}

{% block body_includes %}

    <script src="{{ static_dir }}/js/build/{{ js_main }}"></script>

    {% if google_api_key is not empty %}
    <script type="text/javascript">
        window.googleApiKey = '{{ google_api_key }}';
    </script>
    {% endif %}

    <script type="text/javascript">
        (function() {
            var po = document.createElement('script'); po.type = 'text/javascript'; po.async = true;
            po.src = 'https://apis.google.com/js/client:plusone.js';
            var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(po, s);
        })();
    </script>

    <script type='text/javascript'>
        var _sf_async_config={};
        /** CONFIGURATION START **/
        _sf_async_config.uid = 52361;
        _sf_async_config.domain = 'nationswell.com';
        _sf_async_config.useCanonical = true;
        _sf_async_config.sections = '{{ post.category }}';  //CHANGE THIS
        _sf_async_config.authors = '{{ post.author.display_name }}';    //CHANGE THIS
        /** CONFIGURATION END **/
        (function(){
            function loadChartbeat() {
                window._sf_endpt=(new Date()).getTime();
                var e = document.createElement('script');
                e.setAttribute('language', 'javascript');
                e.setAttribute('type', 'text/javascript');
                e.setAttribute('src', '//static.chartbeat.com/js/chartbeat.js');
                document.body.appendChild(e);
            }
            var oldonload = window.onload;
            window.onload = (typeof window.onload != 'function') ?
                    loadChartbeat : function() { oldonload(); loadChartbeat(); };
        })();
    </script>

    <!-- Quantcast Tag -->
    <script type="text/javascript">
        var _qevents = _qevents || [];

        (function() {
            var elem = document.createElement('script');
            elem.src = (document.location.protocol == "https:" ? "https://secure" : "http://edge") + ".quantserve.com/quant.js";
            elem.async = true;
            elem.type = "text/javascript";
            var scpt = document.getElementsByTagName('script')[0];
            scpt.parentNode.insertBefore(elem, scpt);
        })();

        _qevents.push({
            qacct:"p-tte2VzcqcC-Lz"
        });
    </script>

    <noscript>
        <div style="display:none;">
            <img src="//pixel.quantserve.com/pixel/p-tte2VzcqcC-Lz.gif" border="0" height="1" width="1" alt="Quantcast"/>
        </div>
    </noscript>
    <!-- End Quantcast tag -->
{% endblock %}

{% block extra_includes %}
{% endblock %}

</body>
</html>